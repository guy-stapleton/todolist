{
  "name": "react-redux-template",
  "version": "0.0.1",
  "description": "A template for React andRedux projects",
  "main": "server/index.js",
  "repository": "https://github.com/guy-stapleton/react-redux-template.git",
  "author": "Guy Stapleton <guy.stapleton+1@gmail.com>",
  "license": "MIT",
  "engines": {
      "node": ">=8.9.0"
    },
    "scripts": {
      "dev": "nodemon --watch server & webpack --watch",
      "knex": "knex",
      "migrate": "[ \"$NODE_ENV\" = \"production\" ] && exit 0; knex migrate:latest",
      "migrate:prod": "knex migrate:latest",
      "postinstall": "webpack && npm run migrate",
      "start": "node server",
      "test": "jest",
      "test:watch": "jest --watchAll"
    },
    "jest": {
      "setupFiles": [
        "dotenv/config"
      ]
    },
    "babel": {
      "presets": [
        "env",
        "react",
        "stage-3"
      ]
    },
    "dependencies": {
      "babel-core": "^6.26.0",
      "babel-loader": "^7.1.2",
      "babel-preset-env": "^1.6.1",
      "babel-preset-react": "^6.24.1",
      "babel-preset-stage-3": "^6.24.1",
      "body-parser": "^1.18.2",
      "dotenv": "^5.0.0",
      "express": "^4.16.2",
      "express-jwt": "^5.3.0",
      "jsonwebtoken": "^8.1.1",
      "jwt-decode": "^2.2.0",
      "knex": "^0.14.4",
      "path": "^0.12.7",
      "pg": "^7.4.1",
      "react": "^16.2.0",
      "react-dom": "^16.2.0",
      "react-redux": "^5.0.7",
      "redux": "^3.7.2",
      "redux-thunk": "^2.2.0",
      "sodium": "^2.0.3",
      "superagent": "^3.8.2",
      "webpack": "^3.11.0"
    },
    "devDependencies": {
      "jest": "^22.3.0",
      "nodemon": "^1.15.1",
      "sqlite3": "^3.1.13",
      "supertest": "^3.0.0",
      "webpack-livereload-plugin": "^1.0.0"
    }
  }
